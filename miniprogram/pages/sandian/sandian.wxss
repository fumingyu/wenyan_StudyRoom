

.navbar{
  display: flex;
  background: rgb(255, 255, 255);
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active{
  color: #FFCC00;
}
.item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #FFCC00;
}

.font-title {
  margin: 30rpx 0rpx 20rpx 30rpx;
  font-size: 40rpx;
  font-weight:bold;
}

.room {
  width: 88%;
  height: 200rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
  border: solid 8rpx rgb(150, 154, 160);
  background: rgb(235, 250, 223);
  border-radius: 8%;
}

.room-info {
  display: flex;
  margin-left: 100rpx;
  margin-top: 76rpx;
  font-size: larger;
  font-family: KaiTi, Helvetica, sans-serif;
}

.room-tables {
  margin-left: 60rpx;
}
.table-item {
  margin-top: 30rpx;
}
#room1-table {
  margin-top: -90rpx;
  margin-left: 50rpx;
}
#room1_1, #room1_2 {
   vertical-align:middle;
  width: 100% !important;
  border-radius:25px;
  background-color: rgb(255, 237, 223);
}
#room1_2 {
  margin-top: 15rpx;
}

